int fuel_value = 0; // fuel state 
int fuel_value_inc = 3; // fuel value increment

int temp_value = 80; // fuel state 
int temp_value_inc = -3; // temperature value increment


void setup(void) {
  u8g2.begin(); // start the u8g2 library
  u8g2.setContrast(255); // set the maximum brightness of the display
}

void loop(void) {

  // animate the fuel gauge in the ping-pong motion
  fuel_value = fuel_value + fuel_value_inc;
  if ((fuel_value >= 196) && (fuel_value_inc > 0)) {
    fuel_value_inc = -3;
  } else if ((fuel_value <= 0) && (fuel_value_inc < 0)) {
    fuel_value_inc = 3;
  }

  // animate the temperature gauge in the ping-pong motion
  temp_value = temp_value + temp_value_inc;
  if ((temp_value >= 196) && (temp_value_inc > 0)) {
    temp_value_inc = -3;
  } else if ((temp_value <= 0) && (temp_value_inc < 0)) {
    temp_value_inc = 3;
  }  


  u8g2.firstPage(); // needed for the u8g2 page mode
  do {

     u8g2.setDrawColor(1); // draw in white color
     u8g2.drawXBMP(0, 0, 250, 122, epd_bitmap_reflective_LCD); // draw fullscreen image generated by image2cpp website

     u8g2.setDrawColor(0); // draw in black color
    
     u8g2.drawBox(45 + fuel_value, 16, 196 - fuel_value, 20); // draw black filled box to cover the fuel gauge
     u8g2.drawBox(45 + temp_value, 78, 196 - temp_value, 20); // draw black filled box to cover the temperature gauge


  } while ( u8g2.nextPage() ); // this is required for the u8g2 page mode

}


